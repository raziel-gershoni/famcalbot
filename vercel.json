{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/daily-summary",
      "dest": "/api/daily-summary.ts"
    },
    {
      "src": "/api/tomorrow-summary",
      "dest": "/api/tomorrow-summary.ts"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/health-check",
      "dest": "/api/health-check.ts"
    },
    {
      "src": "/api/privacy",
      "dest": "/api/privacy.ts"
    },
    {
      "src": "/api/refresh-token",
      "dest": "/api/refresh-token.ts"
    },
    {
      "src": "/api/select-calendars",
      "dest": "/api/select-calendars.ts"
    },
    {
      "src": "/api/admin(.*)",
      "dest": "/api/admin.ts"
    }
  ]
}
