{
  "version": 2,
  "installCommand": "npm ci && npx prisma@5.22.0 generate && npx prisma@5.22.0 migrate deploy",
  "routes": [
    {
      "src": "/api/daily-summary",
      "dest": "/api/daily-summary.ts"
    },
    {
      "src": "/api/tomorrow-summary",
      "dest": "/api/tomorrow-summary.ts"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/health-check",
      "dest": "/api/health-check.ts"
    },
    {
      "src": "/api/privacy",
      "dest": "/api/privacy.ts"
    },
    {
      "src": "/api/refresh-token",
      "dest": "/api/refresh-token.ts"
    },
    {
      "src": "/api/select-calendars",
      "dest": "/api/select-calendars.ts"
    },
    {
      "src": "/api/admin(.*)",
      "dest": "/api/admin.ts"
    },
    {
      "src": "/api/dashboard",
      "dest": "/api/dashboard.ts"
    },
    {
      "src": "/api/settings",
      "dest": "/api/settings.ts"
    },
    {
      "src": "/api/admin-panel",
      "dest": "/api/admin-panel.ts"
    }
  ]
}
