{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/daily-summary",
      "dest": "/api/daily-summary.ts"
    },
    {
      "src": "/api/tomorrow-summary",
      "dest": "/api/tomorrow-summary.ts"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/health-check",
      "dest": "/api/health-check.ts"
    },
    {
      "src": "/api/privacy",
      "dest": "/api/privacy.ts"
    },
    {
      "src": "/api/encrypt-tokens",
      "dest": "/api/encrypt-tokens.ts"
    },
    {
      "src": "/api/refresh-token",
      "dest": "/api/refresh-token.ts"
    },
    {
      "src": "/api/admin",
      "dest": "/api/admin/index.ts"
    },
    {
      "src": "/api/admin/oauth-refresh",
      "dest": "/api/admin/oauth-refresh.ts"
    },
    {
      "src": "/api/admin/edit-user",
      "dest": "/api/admin/edit-user.ts"
    },
    {
      "src": "/api/admin/logout",
      "dest": "/api/admin/logout.ts"
    },
    {
      "src": "/api/admin/migrations",
      "dest": "/api/admin/migrations.ts"
    }
  ]
}
