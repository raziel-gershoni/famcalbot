{
  "name": "famcalbot",
  "version": "1.0.0",
  "description": "Family Calendar Telegram Bot with Google Calendar and Claude AI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx scripts/dev.ts",
    "dev:next": "next dev",
    "dev:bot": "tsx src/index.ts",
    "build": "next build",
    "start": "next start",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "type-check": "tsc --noEmit",
    "get-google-token": "tsx scripts/get-google-token.ts",
    "setup-webhook": "tsx scripts/setup-webhook.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "calendar",
    "ai"
