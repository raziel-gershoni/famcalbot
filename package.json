    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.22.0",
    "@upstash/redis": "^1.35.6",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "googleapis": "^164.1.0",
    "hebcal": "^2.3.2",
    "next": "^16.1.1",
    "next-intl": "^4.6.1",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^6.9.1",
    "prisma": "^5.22.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
