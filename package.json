{
  "name": "famcalbot",
  "version": "1.0.0",
  "description": "Family Calendar Telegram Bot with Google Calendar and Claude AI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "vercel-build": "npx prisma migrate deploy",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "get-google-token": "tsx scripts/get-google-token.ts",
    "setup-webhook": "tsx scripts/setup-webhook.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "calendar",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google/generative-ai": "^0.24.1",
    "@upstash/redis": "^1.35.6",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "googleapis": "^164.1.0",
    "hebcal": "^2.3.2",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^6.9.1"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "@types/node": "^24.10.0",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@vercel/node": "^5.5.4",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "form-data": "^4.0.4",
    "tough-cookie": "^6.0.0",
    "esbuild": "^0.25.12",
    "path-to-regexp": "^8.3.0",
    "undici": "^7.16.0"
  }
}
