    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vercel/node": "^5.5.4",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "prettier": "^3.7.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "form-data": "^4.0.4",
    "tough-cookie": "^6.0.0",
    "esbuild": "^0.25.12",
    "path-to-regexp": "^8.3.0",
    "undici": "^7.16.0"
